 // window.media.stop();\\n  // }\\n\\n  playerTouchEnd() {\\n    this.setState({ playerSeekingTouchDown: false });\\n    console.log('onTouchEnd');\\n\\n    if (!this.playerTouchStartMove) {\\n      const newPositionPercent = (this.playerTouchStartPosition - $($('.player_seeking_bar')).offset().left) / (\\n        (parseInt($('.player_seeking_bar').css('width').substring(0, $('.player_seeking_bar').css('width').indexOf(\\\"px\\\")))));\\n\\n      const show_length = ((parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n        + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n          + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n      ) * 1000;\\n\\n      const newTime = show_length * newPositionPercent;\\n      if (newTime <= show_length && newTime >= 0) {\\n        this.setState({\\n          playerTime: newTime\\n        });\\n      }\\n    }\\n\\n    this.playerTouchStartMove = false;\\n\\n\\n    window.media.seekTo(this.state.playerTime);\\n\\n    // const newPositionPercent = Math.round(($('.player_seeking_bul')[0].offsetLeft) / (parseInt($('.player_info_group').css('width').substring(0, $('.player_info_group').css('width').indexOf(\\\"px\\\")))) * 100);\\n\\n    // this.setState({\\n    //   playerTime: (\\n    //     (parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n    //     + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n    //       + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n    //   ) * (newPositionPercent / 100)\\n    // });\\n  }\\n  playerTouchMove(event) {\\n    this.playerTouchStartMove = true;\\n    const newPositionPercent = (event.touches[0].clientX - this.playerTouchStartPosition) / (\\n      (parseInt($('.player_info_group').css('width').substring(0, $('.player_info_group').css('width').indexOf(\\\"px\\\")))));\\n    // console.log('x:', event.touches[0].clientX, ' y:', event.touches[0].clientY)\\n    const show_length = ((parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n      + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n        + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n    ) * 1000;\\n    const newTime = show_length * newPositionPercent + this.state.playerTime;\\n    if (newTime <= show_length && newTime >= 0) {\\n      this.setState({\\n        playerTime: newTime\\n      });\\n      this.playerTouchStartPosition = event.touches[0].clientX;\\n    }\\n    else {\\n      if (newTime >= show_length) {\\n        this.setState({\\n          playerTime: show_length - 1000\\n        });\\n      } else {\\n        this.setState({\\n          playerTime: 1000\\n        });\\n      }\\n    }\\n  }\\n\\n  setIntervalTimePlayer() {\\n    debugger\\n    let fthis = this;\\n    interval = setInterval(function () {\\n      if (fthis.state.playerTime / 1000 <= (\\n        (parseFloat(fthis.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n        + (parseFloat(fthis.state.playerItem.show_length.substring(3, 5) * 60)\\n          + parseFloat(fthis.state.playerItem.show_length.substring(6, 9)))\\n      ) && !fthis.state.playerItem.isLive) {\\n        localStorage.setItem(\\\"playerHistory\\\",\\n          JSON.stringify({\\n            item: fthis.state.playerItem\\n            , time: fthis.state.playerTime\\n          })\\n        );\\n        fthis.setState({\\n          playerTime: fthis.state.playerTime + 100\\n        });\\n      } else {\\n        clearInterval(interval);\\n        localStorage.removeItem(\\\"playerHistory\\\");\\n        //window.media.pause();\\n        //window.media.stop();\\n        //הקטע הסתיים\\n        window.MusicControls.updateIsPlaying(false);\\n      }\\n    }, 100);\\n  }\\n\\n  openMenu() {\\n    this.setState({ menuOpen: true });\\n  }\\n  closeMenu() {\\n    this.setState({ menuOpen: false });\\n  }\\n\\n  //----navigator and animation----///\\n  funAnimationIn1(goToPage, fromPage) {\\n    //--נכנסים דף פנימה Up--//\\n    let callbackFun = () => {\\n      this.funAnimationIn2(goToPage, fromPage);\\n      document.getElementById(goToPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(goToPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n\\n    this.bezy = true;\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n    $('#' + fromPage).css('z-index', 0);\\n    $('#' + goToPage).css('z-index', 89);\\n  }\\n  funAnimationIn2(goToPage, fromPage) {\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('animation', '');\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n  funAnimationOut1(goToPage, fromPage) {\\n    //--חזרה בדפים Down--// \\n\\n    let callbackFun = () => {\\n      this.funAnimationOut2(goToPage, fromPage);\\n      document.getElementById(fromPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(fromPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    this.bezy = true;\\n    $('#' + goToPage).css('z-index', 0);\\n    $('#' + fromPage).css('z-index', 89);\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n  }\\n  funAnimationOut2(goToPage, fromPage) {\\n    $('#' + fromPage).css('animation', '');\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n\\n  ChangePage(goToPage, fromPage) {\\n\\n    if (!this.bezy) {\\n      const fthis = this;\\n\\n      if (fromPage == null) {\\n        fromPage = \\\"\\\" + this.state.historyPages[this.state.historyPages.length - 1] + \\\"\\\";\\n      }\\n\\n      //--זמן האנימציה\\n      const timeAnimation = 250;//250;//ms\\n\\n      if (goToPage !== fromPage) {\\n        //---ניהול חזרות----//\\n        this.bezy = true;\\n        //סיום האפליקציה, סגור\\n        if (this.state.historyPages.length === 1\\n          && goToPage === undefined) {\\n          console.log('\\\"window.navigator.app.exitApp()\\\"');\\n          fthis.state.showSwalLater\\n            ? fthis.state.myChildrens.swal.runSwal(true)\\n            : window.navigator.app.exitApp();\\n        } else {\\n          ///שמור היסטוריה\\n          let new_historyPages = this.state.historyPages;\\n\\n          if (this.listLevelPages[goToPage] <= this.listLevelPages[fromPage]) {\\n            //חוזרים אחורה, מחק את כל הדפים שהרמה שלהם גבוהה משלי.\\n            //new_historyPages.splice(new_historyPages.length - 1, 1);\\n            new_historyPages = new_historyPages.filter((x) => this.listLevelPages[x] < this.listLevelPages[goToPage]);\\n          }\\n          new_historyPages.push(goToPage);\\n          //שמירת שינויים בהיסטוריה\\n          this.setState({\\n            historyPages: new_historyPages\\n          });\\n        }\\n\\n        //----navigator and animation----///\\n\\n        if (this.listLevelPages[goToPage] > this.listLevelPages[fromPage]) {\\n          //--נכנסים דף פנימה Up--//\\n          this.funAnimationIn1(goToPage, fromPage);\\n\\n          if (this.listLevelPages[goToPage] === 1) {\\n            //Up from level 0 to level 1\\n            $('#' + goToPage).css('animation', 'slideInRight ' + timeAnimation + 'ms');\\n\\n          } else { //else if (this.listLevelPages[goToPage] === 2) {\\n            //Up from level 1 to level 2\\n            $('#' + goToPage).css('animation', 'zoomIn ' + timeAnimation + 'ms');\\n          }\\n        } else {\\n          //--חזרה בדפים Down--//   \\n          this.funAnimationOut1(goToPage, fromPage);\\n          if (this.listLevelPages[fromPage] === 1) {\\n            //Down from level 1 to level 0\\n            $('#' + fromPage).css('animation', 'slideOutRight ' + timeAnimation + 'ms');\\n          }\\n          else { //else if (this.listLevelPages[goToPage] === 1) {\\n            //Down from level 2 to level 1\\n            $('#' + fromPage).css('animation', 'zoomOut ' + timeAnimation + 'ms');\\n\\n          }\\n        }\\n        // //עיצוב כפתור חזרה\\n        // if (goToPage === \\\"home\\\") {\\n        //     $('#backInHeader').css('display', \\\"none\\\");\\n        // } else {\\n        //     $('#backInHeader').css('display', \\\"flex\\\");\\n        // }\\n        if (this.listLevelPages[goToPage] < this.listLevelPages[fromPage]) {\\n          if (fromPage === \\\"category\\\") {\\n\\n            if (fthis.state.myChildrens.category.state.ajax) {\\n              fthis.state.myChildrens.category.state.ajax.abort();\\n            }\\n\\n\\n            $('#category').scrollTop(0);\\n            setTimeout(() => {\\n              fthis.state.myChildrens.category.setState({\\n                error: false,\\n                isLoaded: false,\\n                items: []\\n                , hasError: false\\n                , categoryID: 0\\n                , isActive: false\\n                , ajax: false\\n              });\\n            }\\n              , timeAnimation);\\n          }\\n          else if (fromPage === \\\"search\\\") {\\n\\n            if (fthis.state.myChildrens.search.state.ajax)\\n              fthis.state.myChildrens.search.state.ajax.abort();\\n\\n            $('#search').scrollTop(0);\\n            setTimeout(() => {\\n              fthis.state.myChildrens.search.setState({\\n                error: false,\\n                isLoaded: false,\\n                items: []\\n                , hasError: false\\n                , searchText: \\\"\\\"\\n                , isActive: false\\n                , ajax: false\\n              });\\n            }\\n              , timeAnimation);\\n          }\\n\\n          else if (fromPage === \\\"program\\\") {\\n\\n            if (fthis.state.myChildrens.program.state.ajax)\\n              fthis.state.myChildrens.program.state.ajax.abort();\\n\\n            $('#program').scrollTop(0);\\n            setTimeout(() => {\\n              fthis.state.myChildrens.program.setState({\\n                error: false,\\n                isLoaded: false,\\n                items: [],\\n                couterMorItems: 0,\\n                hasError: false,\\n                program: null\\n                , ajax: false\\n              });\\n            }\\n              , timeAnimation);\\n          }\\n        }\\n\\n        this.closeMenu();\\n\\n\\n\\n        //  window.location.href = window.location.href + \\\"#1\\\";\\n      }\\n    }\\n  }\\n  // async CloseApp() {\\n  //   // if (fthis.state.playerMusicControls) {\\n  //   //   window.MusicControls.destroy();\\n  //   //   this.setState({ playerMusicControls: false });\\n  //   // }\\n  //   if (window.media !== undefined){\\n  //     console.log('window.media !== undefined');\\n  //     window.media.stop();\\n  //   }else{\\n  //     console.log('window.media === undefined');\\n  //   }\\n\\n  //   this.bezy = false;\\n  //   return;\\n  // }\\n\\n  componentWillMount() {\\n    // const fthis = this;\\n\\n    if (this.state.playerHistory !== false) {\\n      this.playerPlayNew(this.state.playerHistory.item);\\n    }\\n\\n    window.closeOrBack = () => {\\n      document.getElementById(\\\"backInHeader\\\").click();\\n    }\\n    //\\n    if (window.cordova)\\n      window.plugins.OnDestroyPlugin.setEventListener((event) => {\\n        console.log('OnDestroyPlugin');\\n\\n        // fthis.CloseApp();\\n        // console.log('OnDestroyPluginEnd');\\n      });\\n\\n    //---נעילת מסך\\n    window.screen.orientation.lock('portrait');\\n\\n    //--כפתור חזרה לאנדרואיד\\n    document.addEventListener(\\\"backbutton\\\"\\n      , (e) => {\\n        window.closeOrBack();\\n\\n\\n        //  alert(\\\"Android back button\\\"+JSON.stringify(e));\\n        // navigator.app.exitApp();\\n      }\\n      , false);\\n\\n  }\\n\\n\\n  render() {\\n    const fthis = this;\\n    let header_Height = 60;\\n    let bottom_Height = this.state.playerOpen ? 84 : 0;\\n\\n    let body_Height = this.full_Height - (header_Height + bottom_Height);\\n\\n    let nowPage = this.state.historyPages[this.state.historyPages.length - 1];\\n    return (\\n      [<div id=\\\"backInHeader\\\" key=\\\"backInHeader\\\"\\n        onClick={() => {\\n          fthis.ChangePage(this.state.historyPages[this.state.historyPages.length - 2]\\n            , this.state.historyPages[this.state.historyPages.length - 1])\\n        }}></div>\\n\\n        , <Swal myApp={this} />\\n\\n        , <div key=\\\"body\\\" className=\\\"noScroll body\\\" style={{ //width: window.innerWidth, \\n          height: this.full_Height + \\\"px\\\"\\n        }}>\\n        {[\\n          /* //--header--// */\\n          <div key=\\\"top_section\\\" className=\\\"noScroll top_section\\\" style={{ height: header_Height + \\\"px\\\" }}>\\n            <div className=\\\"noScroll navbar w-nav\\\">\\n              <CheeseburgerMenu\\n                topOffset={header_Height}\\n                bottomOffset={bottom_Height + 0.5}\\n                right={true}\\n                width={230}\\n                isOpen={this.state.menuOpen}\\n                closeCallback={this.closeMenu.bind(this)}>\\n                <MenuContent myParent={this} closeCallback={this.closeMenu.bind(this)} />\\n              </CheeseburgerMenu>\\n              <div style={{ height: header_Height }} className=\\\"hamburger\\\">\\n                <HamburgerMenu\\n                  isOpen={this.state.menuOpen}\\n                  menuClicked={\\n                    this.state.menuOpen\\n                      ? this.closeMenu.bind(this)\\n                      : this.openMenu.bind(this)}\\n                  width={27}\\n                  height={20}\\n                  strokeWidth={2.7}\\n                  rotate={0}\\n                  color='#31b1ca'\\n                  borderRadius={0}\\n                  animationDuration={0.5}\\n                />\\n              </div>\\n            </div>\\n            <div onClick={() => {\\n              if (nowPage === \\\"home\\\") {\\n                if (window.media !== undefined)\\n                  window.media.stop();\\n                window.location.reload();\\n              }\\n              else {\\n                fthis.ChangePage('home', null);\\n                fthis.setState({ historyPages: [\\\"home\\\"] });\\n              }\\n            }} className=\\\"top_logo w-inline-block\\\">\\n              {/* fthis.ChangePage('home', null); fthis.setState({ historyPages: [\\\"home\\\"] });  */}\\n              <img src=\\\"images/logo_radio_turkiz.svg\\\" alt=\\\"\\\" /></div>\\n            <div onClick={() => this.ChangePage(\\\"livePlay\\\", null)} className=\\\"top_live_button w-inline-block\\\">\\n              <div>לשידור החי</div>\\n            </div>\\n          </div>\\n\\n\\n          //* //--body--// */\\n          , <div key=\\\"body_div\\\">\\n            {/* //--home page--// */}\\n            <div key=\\\"home\\\" id=\\\"home\\\" className=\\\"showPage scrollPage\\\" style={{ height: body_Height, top: header_Height }}>\\n              {\\n                nowPage === \\\"home\\\" || fthis.state.historyPages.includes(\\\"home\\\")\\n                  ? <Home myParent={this} />\\n                  : <div></div>\\n              }\\n            </div>\\n\\n            {/* //--schedule--// */}\\n            <div key=\\\"broadcastSchedule\\\" id=\\\"broadcastSchedule\\\" style={{ backgroundColor: \\\"#fff\\\", height: body_Height, top: header_Height }}\\n              className=\\\"hiddenPage\\\">{\\n                nowPage === \\\"broadcastSchedule\\\" || fthis.state.historyPages.includes(\\\"broadcastSchedule\\\")\\n                  ? <BroadcastSchedule myParent={this} />\\n                  : <div></div>\\n              }\\n            </div>\\n\\n            {/* itemsByCategory */}\\n            <div key=\\\"itemsByCategory\\\" id=\\\"itemsByCategory\\\" style={{ backgroundColor: \\\"#fff\\\", height: body_Height, top: header_Height }}\\n              className=\\\"hiddenPage\\\">{\\n                nowPage === \\\"itemsByCategory\\\" || fthis.state.historyPages.includes(\\\"itemsByCategory\\\")\\n                  ? <ItemsByCategory myParent={this} />\\n                  : <div></div>\\n              }\\n            </div>\\n            {/* livePlay */}\\n            <div key=\\\"livePlay\\\" id=\\\"livePlay\\\" style={{ backgroundColor: \\\"#f4f4f4\\\", height: body_Height, top: header_Height }}\\n              className=\\\"hiddenPage\\\">{\\n                nowPage === \\\"livePlay\\\" || fthis.state.historyPages.includes(\\\"livePlay\\\")\\n                  ? <LivePlay myParent={this} />\\n                  : <div></div>\\n              }\\n            </div>\\n            {/* Programs */}\\n            <div key=\\\"programs\\\" id=\\\"programs\\\" style={{ backgroundColor: \\\"#fff\\\", height: body_Height, top: header_Height }}\\n              className=\\\"hiddenPage\\\">{\\n                (nowPage === \\\"programs\\\" || fthis.state.historyPages.includes(\\\"programs\\\")) &&\\n                <Programs myParent={this} />\\n              }\\n            </div>\\n\\n            {/* category */}\\n            <div key=\\\"category\\\" id=\\\"category\\\" style={{ backgroundColor: \\\"#fff\\\", height: body_Height, top: header_Height }} className=\\\"hiddenPage\\\">\\n              <Category myParent={this} />\\n            </div>\\n            {/* search */}\\n            <div key=\\\"search\\\" id=\\\"search\\\" style={{ backgroundColor: \\\"#fff\\\", he