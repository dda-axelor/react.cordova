 from 'jquery';\\n//import './css/jquery.mobile-1.2.1.min.css';\\n\\n//ReactDOM.render(<App />, document.getElementById('root'));\\n\\nwindow.webservices = \\\"\\\";\\nwindow.appVersion = \\\"0.0.1\\\";\\n\\n\\nconst startApp = (cordovaWork) => {\\n  ReactDOM.render(\\n    <div>\\n      <App cordovaWork={cordovaWork} />\\n    </div>,\\n    document.getElementById('root')\\n  );\\n}\\n//\\n//  alert(window.cordova);\\n//  alert(JSON.stringify(window.cordova));\\n\\nif (!window.cordova) {\\n  startApp(false)\\n} else {\\n\\n  document.addEventListener('deviceready', () => {\\n    startApp(true);\\n  }, false);\\n  // alert(deevice.cordova);\\n\\n  // window.device && device.platform === 'iOS'? \\n  //  alert(window.device.platform);\\n  // alert(window.cordova);\\n  //  alert(JSON.stringify(window.cordova));\\n}\\n\\n\\n\\n// If you want your app to work offline and load faster, you can change\\n// unregister() to register() below. Note this comes with some pitfalls.\\n// Learn more about service workers: http://bit.ly/CRA-PWA\\nserviceWorker.unregister();\\n\"]}","code":"(window.webpackJsonp=window.webpackJsonp||[]).push([[0],{13:function(e,a){},16:function(e,a,t){e.exports=t(33)},27:function(e,a,t){},29:function(e,a,t){},31:function(e,a,t){},33:function(e,a,t){\"use strict\";t.r(a);var i=t(0),n=t.n(i),s=t(9),l=t.n(s),r=t(2),c=t(3),o=t(6),m=t(4),d=t(5),g=t(7),u=t(1),p=t.n(u),h=t(11),_=t.n(h),v=t(12),y=t.n(v),E=(t(27),function(e){function a(e){var t;return Object(r.a)(this,a),(t=Object(o.a)(this,Object(m.a)(a).call(this,e))).myApp=t.props.myParent,t.myParent=t.props.myParent,t}return Object(d.a)(a,e),Object(c.a)(a,[{key:\"render\",value:function(){var e=this,a={backgroundColor:\"#31b1ca\",color:\"#fff\"},t=this.myParent.state.historyPages[this.myParent.state.historyPages.length-1];return n.a.createElement(\"div\",{className:\"nav_container w-container\"},n.a.createElement(\"div\",null,n.a.createElement(\"div\",{style:\"home\"===t?a:{},onClick:function(){e.myApp.Change