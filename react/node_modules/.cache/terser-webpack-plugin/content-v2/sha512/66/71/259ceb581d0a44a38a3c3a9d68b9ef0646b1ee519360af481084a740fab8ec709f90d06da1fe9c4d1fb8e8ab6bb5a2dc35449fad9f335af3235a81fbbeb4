w.media.pause() : window.media.play()\\n              : console.log('המתן לסיום הטעינה')\\n            break;\\n          }\\n        case 'music-controls-toggle-play-pause':\\n          {\\n            fthis.state.playerLoaded ?\\n              window.media.pause()\\n              : console.log('המתן לסיום הטעינה');\\n            break;\\n          }\\n        case 'music-controls-play':\\n          {\\n            fthis.state.playerLoaded ?\\n              window.media.play()\\n              : console.log('המתן לסיום הטעינה');\\n            break;\\n          }\\n        case 'music-controls-media-button-previous':\\n          fthis.setState({\\n            playerTime: 0\\n          });\\n          break;\\n        case 'music-controls-media-button-next':\\n          fthis.setState({\\n            playerTime: (fthis.state.playerTime + 10000)\\n          });\\n          break;\\n        case 'music-controls-destroy':\\n          if (fthis.state.playerMusicControls) {\\n            window.MusicControls.destroy(() => {\\n              console.log('destroySuccses ');\\n              if (fthis.state.playerLoaded) {\\n                window.media.pause();\\n                fthis.setState({\\n                  playerOpen: false\\n                  , playerPlay: false\\n                  , playerItem: null\\n                  , playerIsLive: false\\n                  , playerTime: 0\\n                  , playerLoaded: false\\n                  , playerSeekingTouchDown: false\\n                  , playerSeekingPosition: 0\\n                  , playerMusicControls: false\\n                });\\n              }\\n            }, (e) => { console.log('destry onError: ', e); });\\n          }\\n          break;\\n\\n        // // External controls (iOS only)\\n        // case 'music-controls-toggle-play-pause':\\n        //   // Do something\\n        //   break;\\n        case 'music-controls-seek-to':\\n          const seekToInSeconds = JSON.parse(action).position;\\n          window.MusicControls.updateElapsed({\\n            elapsed: seekToInSeconds,\\n            isPlaying: true\\n          });\\n          fthis.setState({\\n            playerTime: (seekToInSeconds * 1000)\\n          });\\n          break;\\n\\n        // Headset events (Android only)\\n        // All media button events are listed below\\n        case 'music-controls-media-button':\\n          // Do something\\n          break;\\n        case 'music-controls-headset-unplugged':\\n          // Do something\\n          break;\\n        case 'music-controls-headset-plugged':\\n          // Do something\\n          break;\\n        default:\\n          break;\\n      }\\n    });\\n\\n    // Start listening for events\\n    // The plugin will run the events function each time an event is fired\\n    window.MusicControls.listen();\\n\\n\\n\\n\\n\\n    if (window.media !== undefined) {\\n      clearInterval(interval);\\n      window.media.stop();\\n      window.media.src = playItem.show_clip_id_full_url;\\n\\n    } else {\\n      window.media = new window.Media(playItem.show_clip_id_full_url\\n        , function () { console.log('mediaSuccess') }\\n        , function (mediaError) { console.log('mediaError:' + JSON.stringify(mediaError)) }\\n        , function (mediaStatus) {\\n          console.log('mediaStatus' + JSON.stringify(mediaStatus));\\n          if (mediaStatus === 4)//stop\\n          {\\n            fthis.setState({\\n              playerPlay: false\\n              , playerLoaded: true\\n              , playerTime: 0\\n            });\\n            clearInterval(interval);\\n            if (fthis.state.playerMusicControls) {\\n              window.MusicControls.updateIsPlaying(false);\\n              // fthis.setState({ playerMusicControls: false });\\n            }\\n\\n            //window.MusicControls.destroy();\\n          }\\n          else if (mediaStatus === 3) {//pause\\n            $('.player_button').css('-webkit-animation', '');\\n            $('.player_button').css('animation', '');\\n            fthis.setState({\\n              playerPlay: false\\n              // , playerLoaded: true\\n            });\\n            clearInterval(interval);\\n            if (fthis.state.playerMusicControls)\\n              window.MusicControls.updateIsPlaying(false);\\n          }\\n          else if (mediaStatus === 2) {//play\\n            $('.player_button').css('-webkit-animation', '');\\n            $('.player_button').css('animation', '');\\n            fthis.setState({\\n              playerPlay: true\\n              , playerLoaded: true\\n            });\\n            fthis.setIntervalTimePlayer();\\n            if (fthis.state.playerMusicControls)\\n              window.MusicControls.updateIsPlaying(true);\\n          }\\n\\n        }\\n      );\\n\\n    }\\n\\n    window.media.play();\\n  }\\n  // playerPlayExists() {\\n  //   window.media.play();\\n  // }\\n  // playerPause() {\\n  // window.media.pause();\\n  // }\\n  // playerStop() {\\n  // window.media.stop();\\n  // }\\n\\n  playerTouchEnd() {\\n    this.setState({ playerSeekingTouchDown: false });\\n    console.log('onTouchEnd');\\n\\n    if (!this.playerTouchStartMove) {\\n      const newPositionPercent = (this.playerTouchStartPosition - $($('.player_seeking_bar')).offset().left) / (\\n        (parseInt($('.player_seeking_bar').css('width').substring(0, $('.player_seeking_bar').css('width').indexOf(\\\"px\\\")))));\\n\\n      const show_length = ((parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n        + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n          + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n      ) * 1000;\\n\\n      const newTime = show_length * newPositionPercent;\\n      if (newTime <= show_length && newTime >= 0) {\\n        this.setState({\\n          playerTime: newTime\\n        });\\n      }\\n    }\\n\\n    this.playerTouchStartMove = false;\\n\\n\\n    window.me