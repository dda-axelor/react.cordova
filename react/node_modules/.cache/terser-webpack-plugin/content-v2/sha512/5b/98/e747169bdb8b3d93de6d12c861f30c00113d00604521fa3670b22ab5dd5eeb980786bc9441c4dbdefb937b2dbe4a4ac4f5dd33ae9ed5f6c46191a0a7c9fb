              else\\r\\n                    return (\\r\\n\\r\\n                        <div>\\r\\n                            <div className=\\\"content_section program\\\">\\r\\n                                <div className=\\\"list_top_picture\\\"><img src={program.program_image} alt={program.program_name} /></div>\\r\\n                                <div className=\\\"program_title_group\\\">\\r\\n                                    <h1 className=\\\"program_main_title\\\">{program.program_name}</h1>\\r\\n                                    <h1 className=\\\"program_main_title\\\">{program.broadcaster}</h1>\\r\\n                                    <div className=\\\"program_title_line\\\" />\\r\\n                                    <div>\\r\\n                                        {program.program_description}\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                            <div className=\\\"content_section w-clearfix\\\" style={{ paddingBottom: fthis.myParent.state.playerOpen ? \\\"160px\\\" : \\\"75px\\\" }}>\\r\\n                                <h2>הפודקאסטים הפופולארים</h2>\\r\\n\\r\\n                                {items.slice(0, 5 * couterMorItems + 5).map(item =>\\r\\n                                    <Item item={item} myParent={fthis} myApp={fthis.myApp} />\\r\\n                                )}\\r\\n\\r\\n                                {((5 * couterMorItems + 5) <= items.length) &&\\r\\n                                    <div className=\\\"add_button w-inline-block\\\">\\r\\n                                        <div onClick={() => fthis.setState({ couterMorItems: fthis.state.couterMorItems + 1 })}>עוד שידורים</div>\\r\\n                                    </div>\\r\\n                                }\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    );\\r\\n            }\\r\\n        }\\r\\n\\r\\n    }\\r\\n}\\r\\n\\r\\n\\r\\n\\r\\nclass Item extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            hasError: false\\r\\n        }\\r\\n        this.myParent = this.props.myParent;\\r\\n        this.myApp = this.props.myApp;\\r\\n    }\\r\\n    componentDidCatch(error, info) {\\r\\n        console.log(\\r\\n            'class Item error at: category.js '\\r\\n            , new Error()\\r\\n            , 'error message:', JSON.stringify(error)\\r\\n            , 'info message:', JSON.stringify(info));\\r\\n        this.setState({ hasError: true });\\r\\n        // logErrorToMyService(error, info);\\r\\n    }\\r\\n\\r\\n\\r\\n    componentWillMount() {\\r\\n\\r\\n    }\\r\\n    render() {\\r\\n        const fthis = this;\\r\\n        if (this.state.hasError) {\\r\\n            return (\\r\\n                <div>error!!</div>\\r\\n            );\\r\\n        } else {\\r\\n            const { item } = this.props;\\r\\n            return (\\r\\n                <div key={item.show_id} className=\\\"item_group w-clearfix\\\">\\r\\n                    <div className=\\\"item_pict_group\\\" style={{ backgroundImage: 'url(' + item.show_image + ')' }}>\\r\\n                        <div className=\\\"item_pict_link w-inline-block\\\"></div>    {/* a */}\\r\\n                        <div className=\\\"item_pict_infogroup\\\">\\r\\n                            <h3>{item.show_name}</h3>\\r\\n                            {/* <div className=\\\"item_cat_group\\\">\\r\\n                                <div className=\\\"item_cat_link\\\">{item.parent_program_name}</div>\\r\\n                                <div className=\\\"item_cat_bullet\\\" />\\r\\n                                <div>{item.broadcaster}</div>\\r\\n                            </div> */}\\r\\n                        </div>\\r\\n                    </div>\\r\\n                    <div className=\\\"item_date_group\\\">\\r\\n                        <div>{item.show_pub_date}</div>\\r\\n                        <div className=\\\"item_time_group\\\">\\r\\n                            <div className=\\\"item_time\\\">\\r\\n                                {Math.round(parseFloat(item.show_length.substring(3, 5) + \\\".\\\" + item.show_length.substring(6, 9)))}\\r\\n                            </div>\\r\\n                            <div>דקות</div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                    <div className=\\\"item_txt\\\" dangerouslySetInnerHTML={{ __html: item.show_description }}>\\r\\n                    </div><div onClick={() => fthis.myApp.setState({\\r\\n                        playerOpen: true\\r\\n                        , playerPlay: true\\r\\n                        , playerItem: item\\r\\n                    })} className=\\\"item_play_link\\\">להאזנה</div>\\r\\n                    <div className=\\\"item_social_group\\\"><div className=\\\"social_link w-inline-block\\\"><img src=\\\"images/ic_facebook.svg\\\" alt=\\\"\\\" /></div>\\r\\n                        <div className=\\\"social_line\\\" /><div className=\\\"social_link w-inline-block\\\"><img src=\\\"images/ic_twitter.svg\\\" alt=\\\"\\\" /></div>\\r\\n                        <div className=\\\"social_line\\\" /><div className=\\\"social_link w-inline-block\\\"><img src=\\\"images/ic_whatsapp.svg\\\" alt=\\\"\\\" /></div></div>\\r\\n                </div>\\r\\n            )\\r\\n        }\\r\\n    }\\r\\n}\",\"import React from 'react';\\nimport $ from 'jquery';\\nimport CheeseburgerMenu from 'cheeseburger-menu';\\nimport HamburgerMenu from 'react-hamburger-menu';\\nimport MenuContent from './menuContent';\\n\\n//--Pages\\nimport Home from \\\"./pages/home\\\";\\nimport BroadcastSchedule from './pages/broadcastSchedule';\\nimport ItemsByCategory from './pages/itemsByCategory';\\nimport LivePlay from './pages/livePlay';\\nimport Category from './pages/category';\\nimport Programs from './pages/programs';\\nimport Program from './pages/program';\\n\\n//-----styles----//\\nimport './css/animate.css';\\nimport \\\"./css/style.css\\\";\\n\\n//----images---//\\n\\n\\nclass App extends React.Component {\\n\\n  constructor(props) {\\n    super(props);\\n    this.state = {\\n      myChildrens: []\\n      , screenPages: []\\n      , historyPages: [\\\"home\\\"]\\n      , menuOpen: false\\n\\n      , playerOpen: false\\n      , playerPlay: false\\n      , playerItem: null\\n\\n    }\\n\\n    this.bezy = false;\\n    this.listLevelPages = [];\\n    this.listLevelPages[\\\"home\\\"] = 0;\\n    this.listLevelPages[\\\"broadcastSchedule\\\"] = 1;\\n    this.listLevelPages[\\\"itemsByCategory\\\"] = 1;\\n    this.listLevelPages[\\\"livePlay\\\"] = 9;\\n    this.listLevelPages[\\\"category\\\"] = 2;\\n    this.listLevelPages[\\\"programs\\\"] = 1;\\n    this.listLevelPages[\\\"program\\\"] = 2;\\n\\n\\n    this.ChangePage = this.ChangePage.bind(this);\\n  }\\n\\n  openMenu() {\\n    this.setState({ menuOpen: true })\\n  }\\n  closeMenu() {\\n    this.setState({ menuOpen: false })\\n  }\\n\\n  //----navigator and animation----///\\n  funAnimationIn1(goToPage, fromPage) {\\n    //--נכנסים דף פנימה Up--//\\n    let callbackFun = () => {\\n      this.funAnimationIn2(goToPage, fromPage);\\n      document.getElementById(goToPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(goToPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n\\n    this.bezy = true;\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n    $('#' + fromPage).css('z-index', 0);\\n    $('#' + goToPage).css('z-index', 89);\\n  }\\n  funAnimationIn2(goToPage, fromPage) {\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('animation', '');\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n  funAnimationOut1(goToPage, fromPage) {\\n    //--חזרה בדפים Down--// \\n\\n    let callbackFun = () => {\\n      this.funAnimationOut2(goToPage, fromPage);\\n      document.getElementById(fromPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(fromPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    this.bezy = true;\\n    $('#' + goToPage).css('z-index', 0);\\n    $('#' + fromPage).css('z-index', 89);\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n  }\\n  funAnimationOut2(goToPage, fromPage) {\\n    $('#' + fromPage).css('animation', '');\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n\\n  ChangePage(goToPage, fromPage) {\\n\\n    const fthis = this;\\n\\n    if (fromPage == null) {\\n      fromPage = \\\"\\\" + this.state.historyPages[this.state.historyPages.length - 1] + \\\"\\\";\\n    }\\n\\n    //--זמן האנימציה\\n    const timeAnimation = 250;//250;//ms\\n\\n    if (goToPage !== fromPage) {\\n      //---ניהול חזרות----//\\n      this.bezy = true;\\n      //סיום האפליקציה, סגור\\n      if (this.state.historyPages.length === 1\\n        && goToPage === undefined) {\\n        window.navigator.app.exitApp();\\n        this.bezy = false;\\n        return;\\n      } else {\\n        ///שמור היסטוריה\\n        let new_historyPages = this.state.historyPages;\\n\\n        if (this.listLevelPages[goToPage] >= this.listLevelPages[fromPage]) {\\n          //נכנסים פנימה, הוסף להסטוריה\\n          new_historyPages.push(goToPage);\\n        } else {\\n          //חוזרים אחורה, מחק את הדף האחרון מההיסטוריה.\\n          new_historyPages.splice(new_historyPages.length - 1, 1);\\n        }\\n        //שמירת שינויים בהיסטוריה\\n        this.setState({\\n          historyPages: new_historyPages\\n        });\\n      }\\n\\n      //----navigator and animation----///\\n\\n      if (this.listLevelPages[goToPage] > this.listLevelPages[fromPage]) {\\n        //--נכנסים דף פנימה Up--//\\n        this.funAnimationIn1(goToPage, fromPage);\\n\\n        if (this.listLevelPages[goToPage] === 1) {\\n          //Up from level 0 to level 1\\n          $('#' + goToPage).css('animation', 'slideInRight ' + timeAnimation + 'ms');\\n\\n        } else { //else if (this.listLevelPages[goToPage] === 2) {\\n          //Up from level 1 to level 2\\n          $('#' + goToPage).css('animation', 'zoomIn ' + timeAnimation + 'ms');\\n        }\\n      } else {\\n        //--חזרה בדפים Down--//   \\n        this.funAnimationOut1(goToPage, fromPage);\\n        if (this.listLevelPages[fromPage] === 1) {\\n          //Down from level 1 to level 0\\n          $('#' + fromPage).css('animation', 'slideOutRight ' + timeAnimation + 'ms');\\n        }\\n        else { //else if (this.listLevelPages[goToPage] === 1) {\\n          //Down from level 2 to level 1\\n          $('#' + fromPage).css('animation', 'zoomOut ' + timeAnimation + 'ms');\\n\\n        }\\n      }\\n      // //עיצוב כפתור חזרה\\n      // if (goToPage === \\\"home\\\") {\\n      //     $('#backInHeader').css('display', \\\"none\\\");\\n      // } else {\\n      //     $('#backInHeader').css('display', \\\"flex\\\");\\n      // }\\n\\n      if (fromPage === \\\"category\\\") {\\n        // $('#').scrollTop(0);\\n        setTimeout(() => {\\n          fthis.state.myChildrens.category.setState({\\n            error: false,\\n            isLoaded: false,\\n            items: []\\n            , hasError: false\\n            , categoryID: 0\\n          });\\n        }\\n          , timeAnimation);\\n      }\\n      if (fromPage === \\\"prog