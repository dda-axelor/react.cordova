Childrens = this.myParent.state.myChildrens;\\r\\n        new_myChildrens[\\\"program\\\"] = this;\\r\\n        this.myParent.setState({\\r\\n            myChildrens: new_myChildrens\\r\\n        });\\r\\n\\r\\n    }\\r\\n\\r\\n\\r\\n    componentDidCatch(error, info) {\\r\\n        console.log(\\r\\n            'class component error at:Program.Program.js ' + this.name\\r\\n            , new Error()\\r\\n            , 'error message:', JSON.stringify(error)\\r\\n            , 'info message:', JSON.stringify(info));\\r\\n        this.setState({ hasError: true });\\r\\n        // logErrorToMyService(error, info);\\r\\n    }\\r\\n\\r\\n\\r\\n    play(program) {\\r\\n\\r\\n        const fthis = this;\\r\\n\\r\\n        fthis.setState({\\r\\n            program: program\\r\\n        });\\r\\n\\r\\n        $.ajax({\\r\\n            url: \\\"https://api.eol.co.il/radio/v1/programs/\\\" + program.program_name,\\r\\n            type: 'GET',\\r\\n            dataType: \\\"json\\\",\\r\\n            contentType: 'application/json; charset = utf-8',\\r\\n            success: function (results) {\\r\\n\\r\\n                fthis.setState({\\r\\n                    isLoaded: true,\\r\\n                    items: results\\r\\n                });\\r\\n            },\\r\\n            error: function (req, status) {//\\r\\n                //    alert(\\\"error ajax\\\"+ req+status)\\r\\n                console.log(req, status);\\r\\n                fthis.setState({\\r\\n                    isLoaded: true,\\r\\n                    error: true\\r\\n                });\\r\\n            }\\r\\n        });\\r\\n    }\\r\\n\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const fthis = this;\\r\\n        const { error, isLoaded, items, hasError, couterMorItems, program } = this.state;\\r\\n\\r\\n        if (hasError) {\\r\\n            return (\\r\\n                <Error myApp={fthis.myApp} showRecomends={false} />\\r\\n            );\\r\\n        } else {\\r\\n            if (error) {\\r\\n