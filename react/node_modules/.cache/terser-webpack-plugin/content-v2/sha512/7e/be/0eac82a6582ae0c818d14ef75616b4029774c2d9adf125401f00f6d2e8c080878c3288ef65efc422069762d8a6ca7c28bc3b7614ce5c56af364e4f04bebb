    );\\r\\n            } else {\\r\\n\\r\\n                if (items.length === 0) { return (<Error myApp={fthis.myApp} title=\\\"לא נמצאו תוצאות לבקשתך\\\" code={404} showRecomends={true} />) }\\r\\n                else\\r\\n                    return (\\r\\n\\r\\n                        <div>\\r\\n                            <div className=\\\"content_section program\\\">\\r\\n                                <div className=\\\"list_top_picture\\\"><img src={program.program_image} alt={program.program_name} /></div>\\r\\n                                <div className=\\\"program_title_group\\\">\\r\\n                                    <h1 className=\\\"program_main_title\\\">{program.program_name}</h1>\\r\\n                                    <h1 className=\\\"program_main_title\\\">{program.broadcaster}</h1>\\r\\n                                    <div className=\\\"program_title_line\\\" />\\r\\n                                    <div>\\r\\n                                        {program.program_description}\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                            <div className=\\\"content_section w-clearfix\\\" style={{ paddingBottom: fthis.myParent.state.playerOpen ? \\\"160px\\\" : \\\"75px\\\" }}>\\r\\n                                <h2>הפודקאסטים הפופולארים</h2>\\r\\n\\r\\n                                {items.slice(0, 5 * couterMorItems + 5).map(item =>\\r\\n                                    <Item item={item} myParent={fthis} myApp={fthis.myApp} />\\r\\n                                )}\\r\\n\\r\\n                                {((5 * couterMorItems + 5) <= items.length) &&\\r\\n                                    <div className=\\\"add_button w-inline-block\\\">\\r\\n                                        <div onClick={() => fthis.setState({ couterMorItems: fthis.state.couterMorItems + 1 })}>עוד שידורים</div>\\r\\n                                    </div>\\r\\n                                }\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    );\\r\\n            }\\r\\n        }\\r\\n\\r\\n    }\\r\\n}\\r\\n\\r\\n\\r\\n\\r\\nclass Item extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            hasError: false\\r\\n        }\\r\\n        this.myParent = this.props.myParent;\\r\\n        this.myApp = this.props.myApp;\\r\\n    }\\r\\n    componentDidCatch(error, info) {\\r\\n        console.log(\\r\\n            'class Item error at: category.js '\\r\\n            , new Error()\\r\\n            , 'error message:', JSON.stringify(error)\\r\\n            , 'info message:', JSON.stringify(info));\\r\\n        this.setState({ hasError: true });\\r\\n        // logErrorToMyService(error, info);\\r\\n    }\\r\\n\\r\\n\\r\\n    componentWillMount() {\\r\\n\\r\\n    }\\r\\n    render() {\\r\\n        const fthis = this;\\r\\n        if (this.state.hasError) {\\r\\n            return (\\r\\n                <div>error!!</div>\\r\\n            );\\r\\n        } else {\\r\\n            const { item } = this.props;\\r\\n            return (\\r\\n                <div key={item.show_id} className=\\\"item_group w-clearfix\\\">\\r\\n                    <div className=\\\"item_pict_group\\\" style={{ backgroundImage: 'url(' + item.show_image + ')' }}>\\r\\n                        <div className=\\\"item_pict_link w-inline-block\\\"></div>    {/* a */}\\r\\n                        <div className=\\\"item_pict_infogroup\\\">\\r\\n                            <h3>{item.show_name}</h3>\\r\\n                            {/* <div className=\\\"item_cat_group\\\">\\r\\n                                <div className=\\\"item_cat_link\\\">{item.parent_program_name}</div>\\r\\n                                <div className=\\\"item_cat_bullet\\\" />\\r\\n                                <div>{item.broadcaster}<