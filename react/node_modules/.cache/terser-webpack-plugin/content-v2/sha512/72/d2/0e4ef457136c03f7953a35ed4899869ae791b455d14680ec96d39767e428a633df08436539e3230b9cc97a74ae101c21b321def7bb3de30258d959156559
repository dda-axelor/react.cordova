\\n    else {\\n      this.setState({\\n        playerOpen: true\\n        //, playerPlay: true\\n        , playerTime: 0\\n        , playerItem: playItem\\n      });\\n      if (window.media !== undefined) {\\n        window.media.pause();\\n        window.media.stop();\\n      }\\n\\n      window.media = new window.Media(playItem === undefined ? fthis.state.playerItem.show_clip_id_full_url : playItem.show_clip_id_full_url\\n        , function () { console.log('mediaSuccess') }\\n        , function (mediaError) { console.log('mediaError:' + JSON.stringify(mediaError)) }\\n        , function (mediaStatus) {\\n          console.log('mediaStatus' + JSON.stringify(mediaStatus));\\n          if (mediaStatus === 4)//stop\\n          {\\n            fthis.setState({\\n              playerPlay: false\\n              , playerLoaded: true\\n              , playerTime: 0\\n            });\\n            clearInterval(interval);\\n          }\\n          if (mediaStatus === 3) {//pause\\n            fthis.setState({\\n              playerPlay: false\\n            });\\n            clearInterval(interval);\\n          }\\n          if (mediaStatus === 2) {//play\\n            fthis.setState({\\n              playerPlay: true\\n              , playerLoaded: true\\n            });\\n            fthis.setIntervalTimePlayer();\\n          }\\n          if (mediaStatus === 1) {//start to loading\\n            fthis.setState({\\n              playerLoaded: false\\n              , playerPlay: false\\n            });\\n          }\\n        }\\n      );\\n    }\\n\\n    if (!this.state.playerPlay) {\\n\\n      window.media.play();\\n      console.log(window.media);\\n      // this.setIntervalTimePlayer();\\n\\n    } else {\\n      //  clearInterval(interval);\\n      window.media.pause();\\n    }\\n\\n  }\\n  setIntervalTimePlayer() {\\n    let fthis = this;\\n    interval = setInterval(function () {\\n      if (fthis.state.playerTime <= (\\n        (parseFloat(fthis.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n        + (parseFloat(fthis.state.playerItem.show_length.substring(3, 5) * 60)\\n          + parseFloat(fthis.state.playerItem.show_length.substring(6, 9)))\\n      )) {\\n        fthis.setState({\\n          playerTime: fthis.state.playerTime + 1\\n        });\\n      } else {\\n        clearInterval(interval);\\n        window.media.pause();\\n        window.media.stop();\\n        //הקטע הסתיים\\n      }\\n    }, 1000);\\n  }\\n\\n  openMenu() {\\n    this.setState({ menuOpen: true });\\n  }\\n  closeMenu() {\\n    this.setState({ menuOpen: false });\\n  }\\n\\n  //----navigator and animation----///\\n  funAnimationIn1(goToPage, fromPage) {\\n    //--נכנסים דף פנימה Up--//\\n    let callbackFun = () => {\\n      this.funAnimationIn2(goToPage, fromPage);\\n      document.getElementById(goToPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(goToPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n\\n    this.bezy = true;\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n    $('#' + fromPage).css('z-index', 0);\\n    $('#' + goToPage).css('z-index', 89);\\n  }\\n  funAnimationIn2(goToPage, fromPage) {\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('animation', '');\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n  funAnimationOut1(goToPage, fromPage) {\\n    //--חזרה בדפים Down--// \\n\\n    let callbackFun = () => {\\n      this.funAnimationOut2(goToPage, fromPage);\\n      document.getElementById(fromPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(fromPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    this.bezy = true;\\n    $('#' + goToPage).css('z-index', 0);\\n    $('#' + fromPage).css('z-index', 89);\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n  }\\n  funAnimationOut2(goToPage, fromPage) {\\n    $('#' + fromPage).css('animation', '');\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n\\n  ChangePage(goToPage, fromPage) {\\n\\n    if (!this.bezy) {\\n      const fthis = this;\\n\\n      if (fromPage == null) {\\n        fromPage = \\\"\\\" + this.state.historyPages[this.state.historyPages.length - 1] + \\\"\\\";\\n      }\\n\\n      //--זמן האנימציה\\n      const timeAnimation = 250;//250;//ms\\n\\n      if (goToPage !== fromPage) {\\n        //---ניהול חזרות----//\\n        this.bezy = true;\\n        //סיום האפליקציה, סגור\\n        if (this.state.historyPages.length === 1\\n          && goToPage === undefined) {\\n          window.navigator.app.exitApp();\\n          this.bezy = false;\\n          return;\\n        } else {\\n          ///שמור היסטוריה\\n          let new_historyPages = this.state.historyPages;\\n\\n          if (this.listLevelPages[goToPage] >= this.listLevelPages[fromPage]) {\\n            //נכנסים פנימה, הוסף להסטוריה\\n            new_historyPages.push(goToPage);\\n          } else {\\n            //חוזרים אחורה, מחק את הדף האחרון מההיסטוריה.\\n            new_historyPages.splice(new_historyPages.length - 1, 1);\\n          }\\n          //שמירת שינויים בהיסטוריה\\n          this.setState({\\n            historyPages: new_historyPages\\n          });\\n        }\\n\\n        //----navigator and animation----///\\n\\n        if (this.listLevelPages[goToPage] > this.listLevelPages[fromPage]) {\\n          //--נכנסים דף פנימה Up--//\\n          this.funAnimationIn1(goToPage, fromPage);\\n\\n          if (this.listLevelPages[goToPage] === 1) {\\n            //Up from level 0 to level 1\\n            $('#' + goToPage).css('animation', 'slideInRight ' + timeAnimation + 'ms');\\n\\n          } else { //else if (this.listLevelPages[goToPage] === 2) {\\n            //Up from level 1 to level 2\\n            $('#' + goToPage).css('animation', 'zoomIn ' + timeAnimation + 'ms');\\n          }\\n        } else {\\n          //--חזרה בדפים Down--//   \\n          this.funAnimationOut1(goToPage, fromPage);\\n          if (this.listLevelPages[fromPage] === 1) {\\n            //Down from level 1 to level 0\\n            $('#' + fromPage).css('animation', 'slideOutRight ' + timeAnimation + 'ms');\\n          }\\n          else { //else if (this.listLevelPages[goToPage] === 1) {\\n            //Down from level 2 to level 1\\n            $('#' + fromPage).css('animation', 'zoomOut ' + timeAnimation + 'ms');\\n\\n          }\\n        }\\n        // //עיצוב כפתור חזרה\\n        // if (goToPage === \\\"home\\\") {\\n        //     $('#backInHeader').css('display', \\\"none\\\");\\n        // } else {\\n        //     $('#backInHeader').css('display', \\\"flex\\\");\\n        // }\\n        if (this.listLevelPages[goToPage] < this.listLevelPages[fromPage]) {\\n          if (fromPage === \\\"category\\\") {\\n            // $('#').scrollTop(0);\\n            setTimeout(() => {\\n              fthis.state.myChildrens.category.setState({\\n                error: false,\\n                isLoaded: false,\\n                items: []\\n                , hasError: false\\n                , categoryID: 0\\n              });\\n            }\\n              , timeAnimation);\\n          }\\n          if (fromPage === \\\"program\\\") {\\n            // $('#').scrollTop(0);\\n            setTimeout(() => {\\n              fthis.state.myChildrens.program.setState({\\n                error: false,\\n                isLoaded: false,\\n         