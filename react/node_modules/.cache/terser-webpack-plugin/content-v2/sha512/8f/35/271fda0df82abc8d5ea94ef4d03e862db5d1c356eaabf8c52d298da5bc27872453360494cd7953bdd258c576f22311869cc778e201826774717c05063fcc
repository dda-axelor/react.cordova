   case 'music-controls-media-button-play-pause':\\n          {\\n            fthis.state.playerLoaded ?\\n              fthis.state.playerPlay ? window.media.pause() : window.media.play()\\n              : console.log('המתן לסיום הטעינה')\\n            break;\\n          }\\n        case 'music-controls-toggle-play-pause':\\n          {\\n            fthis.state.playerLoaded ?\\n              window.media.pause()\\n              : console.log('המתן לסיום הטעינה');\\n            break;\\n          }\\n        case 'music-controls-play':\\n          {\\n            fthis.state.playerLoaded ?\\n              window.media.play()\\n              : console.log('המתן לסיום הטעינה');\\n            break;\\n          }\\n        case 'music-controls-media-button-previous':\\n          fthis.setState({\\n            playerTime: 0\\n          });\\n          break;\\n        case 'music-controls-media-button-next':\\n          fthis.setState({\\n            playerTime: (fthis.state.playerTime + 10000)\\n          });\\n          break;\\n        case 'music-controls-destroy':\\n          window.MusicControls.destroy(() => {\\n            console.log('destroySuccses ');\\n            if (fthis.state.playerLoaded) {\\n              window.media.pause();\\n              fthis.setState({\\n                playerOpen: false\\n                , playerPlay: false\\n                , playerItem: null\\n                , playerIsLive: false\\n                , playerTime: 0\\n                , playerLoaded: false\\n                , playerSeekingTouchDown: false\\n                , playerSeekingPosition: 0\\n                , playerMusicControls: false\\n              });\\n            }\\n          }, (e) => { console.log('destry onError: ', e); });\\n          break;\\n\\n        // // External controls (iOS only)\\n        // case 'music-controls-toggle-play-pause':\\n        //   // Do something\\n        //   break;\\n        case 'music-controls-seek-to':\\n          const seekToInSeconds = JSON.parse(action).position;\\n          window.MusicControls.updateElapsed({\\n            elapsed: seekToInSeconds,\\n            isPlaying: true\\n          });\\n          fthis.setState({\\n            playerTime: (seekToInSeconds * 1000)\\n          });\\n          break;\\n\\n        // Headset events (Android only)\\n        // All media button events are listed below\\n        case 'music-controls-media-button':\\n          // Do something\\n          break;\\n        case 'music-controls-headset-unplugged':\\n          // Do something\\n          break;\\n        case 'music-controls-headset-plugged':\\n          // Do something\\n          break;\\n        default:\\n          break;\\n      }\\n    });\\n\\n    // Start listening for events\\n    // The plugin will run the events function each time an event is fired\\n    window.MusicControls.listen();\\n\\n\\n\\n\\n\\n    if (window.media !== undefined) {\\n      clearInterval(interval);\\n      window.media.stop();\\n      window.media.src = playItem.show_clip_id_full_url;\\n\\n    } else {\\n      window.media = new window.Media(playItem.show_clip_id_full_url\\n        , function () { console.log('mediaSuccess') }\\n        , function (mediaError) { console.log('mediaError:' + JSON.stringify(mediaError)) }\\n        , function (mediaStatus) {\\n          console.log('mediaStatus' + JSON.stringify(mediaStatus));\\n          if (mediaStatus === 4)//stop\\n          {\\n            fthis.setState({\\n              playerPlay: false\\n              , playerLoaded: true\\n              , playerTime: 0\\n            });\\n            clearInterval(interval);\\n            if (fthis.state.playerMusicControls)\\n              window.MusicControls.updateIsPlaying(false);\\n          }\\n          else if (mediaStatus === 3) {//pause\\n            $('.player_button').css('-webkit-animation', '');\\n            $('.player_button').css('animation', '');\\n            fthis.setState({\\n              playerPlay: false\\n              // , playerLoaded: true\\n            });\\n            clearInterval(interval);\\n            if (fthis.state.playerMusicControls)\\n              window.MusicControls.updateIsPlaying(false);\\n          }\\n          else if (mediaStatus === 2) {//play\\n            $('.player_button').css('-webkit-animation', '');\\n            $('.player_button').css('animation', '');\\n            fthis.setState({\\n              playerPlay: true\\n              , playerLoaded: true\\n            });\\n            fthis.setIntervalTimePlayer();\\n            if (fthis.state.playerMusicControls)\\n              window.MusicControls.updateIsPlaying(true);\\n          }\\n\\n        }\\n      );\\n\\n    }\\n\\n    window.media.play();\\n  }\\n  // playerPlayExists() {\\n  //   window.media.play();\\n  // }\\n  // playerPause() {\\n  // window.media.pause();\\n  // }\\n  // playerStop() {\\n  // window.media.stop();\\n  // }\\n\\n  playerTouchEnd() {\\n    this.setState({ playerSeekingTouchDown: false });\\n    console.log('onTouchEnd');\\n\\n    if (!this.playerTouchStartMove) {\\n      const newPositionPercent = (this.playerTouchStartPosition - $($('.player_seeking_bar')).offset().left) / (\\n        (parseInt($('.player_seeking_bar').css('width').substring(0, $('.player_seeking_bar').css('width').indexOf(\\\"px\\\")))));\\n\\n      const show_length = ((parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n        + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n          + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n      ) * 1000;\\n\\n      const newTime = show_length * newPositionPercent;\\n      if (newTime <= show_length && newTime >= 0) {\\n        this.setState({\\n          playerTime: newTime\\n        });\\n      }\\n    }\\n\\n    this.playerTouchStartMove = false;\\n\\n\\n    window.media.seekTo(this.state.playerTime);\\n\\n    // const newPositionPercent = Math.round(($('.player_seeking_bul')[0].offsetLeft) / (parseInt($('.player_info_group').css('width').substring(0, $('.player_info_group').css('width').indexOf(\\\"px\\\")))) * 100);\\n\\n    // this.setState({\\n    //   playerTime: (\\n    //     (parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n    //     + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n    //       + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n    //   ) * (newPositionPercent / 100)\\n    // });\\n  }\\n  playerTouchMove(event) {\\n    this.playerTouchStartMove = true;\\n    const newPositionPercent = (event.touches[0].clientX - this.playerTouchStartPosition) / (\\n      (parseInt($('.player_info_group').css('width').substring(0, $('.player_info_group').css('width').indexOf(\\\"px\\\")))));\\n    // console.log('x:', event.touches[0].clientX, ' y:', event.touches[0].clientY)\\n    const show_length = ((parseFloat(this.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n      + (parseFloat(this.state.playerItem.show_length.substring(3, 5) * 60)\\n        + parseFloat(this.state.playerItem.show_length.substring(6, 9)))\\n    ) * 1000;\\n    const newTime = show_length * newPositionPercent + this.state.playerTime;\\n    if (newTime <= show_length && newTime >= 0) {\\n      this.setState({\\n        playerTime: newTime\\n      });\\n      this.playerTouchStartPosition = event.touches[0].clientX;\\n    }\\n    else {\\n      if (newTime >= show_length) {\\n        this.setState({\\n          playerTime: show_length - 1000\\n        });\\n      } else {\\n        this.setState({\\n          playerTime: 1000\\n        });\\n      }\\n    }\\n  }\\n\\n  setIntervalTimePlayer() {\\n    let fthis = this;\\n    interval = setInterval(function () {\\n      if (fthis.state.playerTime / 1000 <= (\\n        (parseFloat(fthis.state.playerItem.show_length.substring(0, 2)) * 60 * 60)\\n        + (parseFloat(fthis.state.playerItem.show_length.substring(3, 5) * 60)\\n          + parseFloat(fthis.state.playerItem.show_length.substring(6, 9)))\\n      )) {\\n        fthis.setState({\\n          playerTime: fthis.state.playerTime + 100\\n        });\\n      } else {\\n        clearInterval(interval);\\n        window.media.pause();\\n        window.media.stop();\\n        //הקטע הסתיים\\n      }\\n    }, 100);\\n  }\\n\\n  openMenu() {\\n    this.setState({ menuOpen: true });\\n  }\\n  closeMenu() {\\n    this.setState({ menuOpen: false });\\n  }\\n\\n  //----navigator and animation----///\\n  funAnimationIn1(goToPage, fromPage) {\\n    //--נכנסים דף פנימה Up--//\\n    let callbackFun = () => {\\n      this.funAnimationIn2(goToPage, fromPage);\\n      document.getElementById(goToPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(goToPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n\\n    this.bezy = true;\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n    $('#' + fromPage).css('z-index', 0);\\n    $('#' + goToPage).css('z-index', 89);\\n  }\\n  funAnimationIn2(goToPage, fromPage) {\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + goToPage).css('animation', '');\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n  funAnimationOut1(goToPage, fromPage) {\\n    //--חזרה בדפים Down--// \\n\\n    let callbackFun = () => {\\n      this.funAnimationOut2(goToPage, fromPage);\\n      document.getElementById(fromPage).removeEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    };\\n    document.getElementById(fromPage).addEventListener(\\\"webkitAnimationEnd\\\", callbackFun);\\n    this.bezy = true;\\n    $('#' + goToPage).css('z-index', 0);\\n    $('#' + fromPage).css('z-index', 89);\\n    $('#' + goToPage).removeClass('hiddenPage');\\n    $('#' + goToPage).addClass('scrollPage showPage');\\n  }\\n  funAnimationOut2(goToPage, fromPage) {\\n    $('#' + fromPage).css('animation', '');\\n    $('#' + goToPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).css('z-index', \\\"\\\");\\n    $('#' + fromPage).removeClass('showPage');\\n    $('#' + fromPage).removeClass('scrollPage');\\n    $('#' + fromPage).addClass('hiddenPage');\\n    this.bezy = false;\\n  }\\n\\n\\n  ChangePage(goToPage, fromPage) {\\n\\n    if (!this.bezy) {\\n      const fthis = this;\\n\\n      if (fromPage == null) {\\n        fromPage = \\\"\\\" + this.state.historyPages[this.state.historyPages.length - 1] + \\\"\\\";\\n      }\\n\\n      //--זמן האנימציה\\n      const timeAnimation = 250;//250;//ms\\n\\n      if (goToPage !== fromPage) {\\n        //---ניהול חזרות----//\\n        this.bezy = true;\\n        //סיום האפליקציה, סגור\\n        if (this.state.historyPages.length === 1\\n          && goToPage === undefined) {\\n          if (fthis.state.playerMusicControls)\\n            window.MusicControls.destroy();\\n\\n          window.navigator.app.exitApp();\\n          this.bezy = false;\\n          return;\\n        } else {\\n          ///שמור היסטוריה\\n          let new_historyPages = this.state.historyPages;\\n\\n          if (this.listLevelPages[goToPage] <= this.listLevelPages[fromPage]) {\\n            //חוזרים אחורה, מחק את כל הדפים שהרמה שלהם גבוהה משלי.\\n            //new_historyPages.splice(new_historyPages.length - 1, 1);\\n            new_historyPages = new_historyPages.filter((x) => this.listLevelPages[x] < this.listLevelPages[goToPage]);\\n          }\\n          new_historyPages.push(goToPage);\\n          //שמירת שינויים בהיסטוריה\\n          this.setState({\\n            historyPages: new_historyPages\\n          });\\n        }\\n\\n        //----navigator and animation----///\\n\\n        if (this.listLevelPages[goToPage] > this.listLevelPages[fromPage]) {\\n          //--נכנסים דף פנימה Up--//\\n          this.funAnimationIn1(goToPage, fromPage);\\n\\n          if (this.listLevelPages[goToPage] === 1) {\\n            //Up from level 0 to level 1\\n            $('#' + goToPage).css('animation', 'slideInRight ' + timeAnimation + 'ms');\\n\\n          } else { //else if (this.listLevelPages[goToPage] === 2) {\\n            //Up from level 1 to level 2\\n            $('#' + goToPage).css('animation', 'zoomIn ' + timeAnimation + 'ms');\\n          }\\n        } else {\\n          //--חזרה בדפים Down--//   \\n          this.funAnimationOut1(goToPage, fromPage);\\n          if (this.listLevelPages[fromPage] === 1) {\\n            //Down from level 1 to level 0\\n            $('#' + fromPage).css('animation', 'slideOutRight ' + timeAnimation + 'ms');\\n          }\\n          else { //else if (this.listLevelPages[goToPage] === 1) {\\n            //Down from level 2 to level 1\\n            $('#' + fromPage).css('animation', 'zoomOut ' + timeAnimation + 'ms');\\n\\n          }\\n        }\\n        // //עיצוב כפתור חזרה\\n        // if (goToPage === \\\"home\\\") {\\n        //     $('#backInHeader').css('display', \\\"none\\\");\\n        // } else {\\n        //     $('#backInHeader').css('display', \\\"flex\\\");\\n        // }\\n        if (this.listLevelPages[goToPage] < this.listLevelPages[fromPage]) {\\n          if (fromPage === \\\"category\\\") {\\n\\n            if (fthis.state.myChildrens.category.state.ajax)\\n              fthis.state.myChildrens.category.state.ajax.abort();\\n\\n            $('#category').scrollTop(0);\\n            setTimeout(() => {\\n              fthis.state.myChildrens.category.setState({\\n                error: false,\\n                isLoaded: false,\\n                items: []\\n                , hasError: false\\n                , categoryID: 0\\n                , isActive: false\\n              });\\n            }\\n              , timeAnimation);\\n          }\\n          if (