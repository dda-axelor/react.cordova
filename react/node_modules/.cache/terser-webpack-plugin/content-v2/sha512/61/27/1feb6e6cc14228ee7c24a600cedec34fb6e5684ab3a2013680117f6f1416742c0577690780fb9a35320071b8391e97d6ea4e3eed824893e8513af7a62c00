ByCategory\\\"] = 1;\\n    this.listLevelPages[\\\"livePlay\\\"] = 9;\\n    this.listLevelPages[\\\"category\\\"] = 2;\\n    this.listLevelPages[\\\"programs\\\"] = 1;\\n    this.listLevelPages[\\\"program\\\"] = 2;\\n\\n\\n    this.ChangePage = this.ChangePage.bind(this);\\n    //  this.playPause = this.playPause.bind(this);\\n\\n\\n  }\\n\\n  playerPlayNew(playItem) {\\n\\n    let fthis = this;\\n\\n    this.setState({\\n      playerOpen: true\\n      //, playerPlay: true\\n      , playerTime: 0\\n      , playerItem: playItem\\n    });\\n    if (window.media !== undefined) {\\n      clearInterval(interval);\\n      window.media.stop();\\n      window.media.src = playItem.show_clip_id_full_url;\\n\\n    } else {\\n      window.media = new window.Media(playItem.show_clip_id_full_url\\n        , function () { console.log('mediaSuccess') }\\n        , function (mediaError) { console.log('mediaError:' + JSON.stringify(mediaError)) }\\n        , function (mediaStatus) {\\n          console.log('mediaStatus' + JSON.stringify(mediaStatus));\\n          if (mediaStatus === 4)//stop\\n          {\\n            fthis.setState({\\n              playerPlay: false\\n              , playerLoaded: true\\n              , playerTime: 0\\n            });\\n            clearInterval(interval);\\n          }\\n          else if (mediaStatus === 3) {//pause\\n            $('.player_button').css('-webkit-animation', '');\\n            $('.player_button').css('animation', '');\\n            fthis.setState({\\n              playerPlay: false\\n              , playerLoaded: true\\n            });\\n            clearInterval(interval);\\n          }\\n          else if (mediaStatus === 2) {//play\\n            $('.player_button').css('-webkit-animation', '');\\n            $('.player_button').css('animation', '');\\n            fthis.setState({\\n              playerPlay: true\\n              , playerLoaded: true\\n            });\\n            fthis.setIntervalTimePlayer();\\n          }\\